{
  "name": "vscode-doc-bot-extension",
  "displayName": "DocBot",
  "description": "Helps summarize documentation and writes code looking at documentation you need",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-doc-bot-extension.askQuestion",
        "title": "Ask a question"
      },
      {
        "command": "vscode-doc-bot-extension.error",
        "title": "Bye World"
      },
      {
        "command": "vscode-doc-bot-extension.callBot",
        "title": "Call Bot"
      }
    ],
    "menus":{
      "editor/context":[
        {
        "command":"vscode-doc-bot-extension.callBot",
        "when":"editorTextFocus && editorHasSelection",
        "group":"navigation"
      }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "mocha": "^10.6.0",
    "typescript": "^5.4.5",
    "vscode": "^1.1.37",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
